{
  "schemaVersion": 1,
  "id": "opti",
  "version": "${version}",
  "name": "Opti Client",
  "description": "Opti Client - The Ultimate Minecraft Enhancement",
  "authors": [
    "Opti Client Team"
  ],
  "contact": {
    "homepage": "https://github.com/OptiClient",
    "sources": "https://github.com/OptiClient/Opti-Client",
    "issues": "https://github.com/OptiClient/Opti-Client/issues"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/opti/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.opticlient.OptiInitializer",
      "net.opticlient.test.OptiE2ETestClient"
    ]
  },
  "mixins": [
    "opti.mixins.json"
  ],
  "accessWidener": "opti.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.13",
    "fabric-api": ">=0.126.1",
    "minecraft": "~1.21.6-beta.3",
    "java": ">=21"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "vulkanmod": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.donate": "https://github.com/OptiClient/Opti-Client",
        "modmenu.twitter": "https://github.com/OptiClient",
        "modmenu.wiki": "https://github.com/OptiClient/Opti-Client/wiki",
        "Forum": "https://github.com/OptiClient/Opti-Client/discussions"
      }
    }
  }
}
